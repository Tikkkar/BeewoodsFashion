{
  "version": 2,
  "env": {
    "DISABLE_ESLINT_PLUGIN": "true",
    "CI": "false"
  },
  "build": {
    "env": {
      "REACT_APP_OPENROUTER_API_KEY": "$REACT_APP_OPENROUTER_API_KEY",
      "REACT_APP_SUPABASE_URL": "$VITE_SUPABASE_URL",
      "REACT_APP_SUPABASE_ANON_KEY": "$VITE_SUPABASE_ANON_KEY"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}